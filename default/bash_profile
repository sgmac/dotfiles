# Pyenv
# Adding  profile and bashrc
[[ -e $HOME/.bashrc ]] && source $HOME/.bashrc
if which pyenv-virtualenv-init > /dev/null; then eval "$(pyenv virtualenv-init -)"; fi

# The next line updates PATH for the Google Cloud SDK.
if [ -f '/home/sgm/.src/google-cloud-sdk/path.bash.inc' ]; then source '/home/sgm/.src/google-cloud-sdk/path.bash.inc'; fi

# The next line enables shell command completion for gcloud.
if [ -f '/home/sgm/.src/google-cloud-sdk/completion.bash.inc' ]; then source '/home/sgm/.src/google-cloud-sdk/completion.bash.inc'; fi

export PATH="$HOME/.cargo/bin:$PATH:/sbin"


export PATH="$HOME/.cargo/bin:$PATH"
