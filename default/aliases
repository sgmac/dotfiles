# Docker
alias dcb='docker-compose build'
alias dcu='docker-compose up'
alias dcd='docker-compose down'

# Servers
alias hlab='ssh h.wipefs.com'
alias chefserver='ssh root@chef.wipefs.com'


# Misce
alias soundext='pacmd set-default-sink alsa_output.usb-XtremPro_inc_._XtremPro_X1_USB_DAC-00.analog-stereo'
alias sounddef='pacmd set-default-sink alsa_output.pci-0000_00_1f.3.analog-stereo'
alias private='sudo cryptsetup luksOpen /dev/mapper/vg--sys-private private'
alias suspend='i3lock  -c 111111 && sudo pm-suspend'
